{
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [{ "key": "Content-Type", "value": "application/json" }]
    }
  ],
  "rewrites": [
    { "source": "/station", "destination": "/station.html" },
    { "source": "/station/:path*", "destination": "/station.html" }
  ]
}